{
    "name": "extalion/php-strict-array",
    "description": "Generator for generic arrays (vector, map)",
    "keywords": ["array", "vector", "map", "collections", "strict", "strict-types"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Damian Glinkowski",
            "email": "damian@d0niek.pl"
        }
    ],
    "autoload": {
        "classmap": [
            "arrays/"
        ],
        "files": [
            "functions/sarray_filter.php",
            "functions/sarray_key_exists.php",
            "functions/sarray_keys.php",
            "functions/sarray_search.php",
            "functions/sarray_values.php",
            "functions/throw_type_error.php",
            "Pair.php",
            "StrictArray.php"
        ],
        "psr-4": {
            "eXtalion\\PhpStrictArray\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "eXtalion\\PhpStrictArray\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "extalion/php-enum": "^0.1",
        "symfony/console": "^5.0",
        "twig/twig": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "bin": [
        "bin/php-sa"
    ]
}
