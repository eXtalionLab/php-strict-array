#!/usr/bin/env php
<?php
declare(strict_types=1);

require_once __DIR__ . '/../vendor/autoload.php';
$arrayGenerator = new \eXtalion\PhpStrictArray\Service\StrictArrayGenerator();
$command = new \eXtalion\PhpStrictArray\Command\StrictArrayGenerator(
    $arrayGenerator
);
$app = new \Symfony\Component\Console\Application();
$app->add($command);
$app->setDefaultCommand($command->getName());
$app->run();
